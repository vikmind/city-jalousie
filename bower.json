{
	"name": "gulp-frontend-boilerplate",
	"version": "0.1.2",
	"homepage": "https://github.com/fatuk/gulp-frontend-boilerplate",
	"authors": [
		"fatuk <fatukk@gmail.com>",
		"vikmind <viktor.marchenko@gmail.com"
	],
	"moduleType": [
		"amd"
	],
	"license": "MIT",
	"private": true,
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"test",
		"tests"
	],
	"overrides": {
		"angular-owl-carousel1": {
			"main": "src/angular-owl-carousel.js"
		},
		"owl-carousel": {
			"main": "owl-carousel/owl.carousel.js"
		},
		"angularjs-slider": {
			"main": "dist/rzslider.js"
		},
		"normalize.less": {
			"ignore": [
				"normalize.less",
				"normalize.css"
			]
		},
		"angular-youtube-mb": {
			"main": "dist/angular-youtube-embed.min.js"
		}
	},
	"dependencies": {
		"jquery": "1.11.3"
	},
	"devDependencies": {
		"svg4everybody": "~2.0.0"
	}
}
